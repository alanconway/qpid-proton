/// @cond INTERNAL

/// @file
///
/// Configuration macros.

/// True if the library was built with C++11 support.
#cmakedefine01 PN_CPP_SUPPORTS_CPP11

/// True if the library was built with multi-threading support.
/// See @ref container::MULTI_THREADED
#cmakedefine01 PN_CPP_SUPPORTS_THREADS

// Detect the compiler version and enable C++11 API features if possible.
#ifndef PN_CPP_HAS_CPP11
# if defined(__cplusplus) && __cplusplus >= 201103 && PN_CPP_SUPPORTS_CPP11
#  define PN_CPP_HAS_CPP11 1
# else
#  define PN_CPP_HAS_CPP11 0
# endif
#endif

#if PN_CPP_HAS_CPP11
# define PN_CPP_OVERRIDE override
#else
# define PN_CPP_OVERRIDE
#endif


#endif // PROTON_INTERNAL_CONFIG_HPP
